{
  "name": "dog-breeds-dashboard",
  "version": "1.0.0",
  "description": "Dog Breeds Dashboard",
  "scripts": {
    "start": "webpack-dev-server --colors --progress --inline --hot --env=development --config=webpack/config.js",
    "test": "jest",
    "test:coverage": "yarn test -- --coverage",
    "release": "cross-env BABEL_ENV=production NODE_ENV=production webpack -p --colors --progress --env=production --config=webpack/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renerbaffa/dog-breeds-dashboard.git"
  },
  "author": "Rener Baffa da Silva (renerbaffa@gmail.com)",
  "devDependencies": {
    "axios-mock-adapter": "^1.11.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "postcss-color-function": "^4.0.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-mathjs": "^1.0.0",
    "postcss-nested": "^2.0.2",
    "postcss-sassy-mixins": "^2.1.0",
    "postcss-simple-vars": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "cacheDirectory": "/tmp/jest-cache-folder",
    "collectCoverageFrom": [
      "src/**.{js,jsx}"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "verbose": true
  }
}
